{
    "name": "armonia-tech/goutte",
    "type": "application",
    "description": "A simple PHP Web Scraper",
    "keywords": ["scraper"],
    "homepage": "https://github.com/armonia-tech/Goutte",
    "license": "MIT",
    "authors": [
        {
            "name": "Armonia Tech",
            "email": "huanyong.chan@armonia-tech.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/browser-kit": "~2.1|~3.0|~4.0",
        "symfony/css-selector": "~2.1|~3.0|~4.0",
        "symfony/dom-crawler": "~2.1|~3.0|~4.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3 || ^4"
    },
    "autoload": {
        "psr-4": { "Goutte\\": "Goutte" },
        "exclude-from-classmap": ["Goutte/Tests"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
